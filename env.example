# Webflow API Configuration
# Copy this file to .env and fill in your actual values

# Your Webflow API Token
# Get it from: https://webflow.com/dashboard/account/integrations
WEBFLOW_API_TOKEN=your_api_token_here

# Your Webflow Site ID
# Find it in your site settings or via the Webflow API
WEBFLOW_SITE_ID=your_site_id_here

# Your Webflow Collection ID
# The ID of the CMS collection where you want to post items
WEBFLOW_COLLECTION_ID=your_collection_id_here

# Optional: Webflow Folder ID for organizing uploaded assets
# Leave empty to upload to root folder
WEBFLOW_FOLDER_ID=

